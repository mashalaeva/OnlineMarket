@model App.Web.Models.UserProfileModel

@await Html.PartialAsync("_UserView", Model.User)

<h1>Профиль пользователя</h1>
<div class="row">
    <div class="col-7">
        <div class="list-group">

            <div class="list-group-item d-flex justify-content-start">
                <div class="px-3 mx-3">Имя: @Model.User.Name </div>
            </div>
            <div class="list-group-item d-flex justify-content-start">
                <div class="px-3 mx-3">Фамилия: @Model.User.Surname</div>
            </div>
            <div class="list-group-item d-flex justify-content-start">
                <div class="px-3 mx-3">Email: @Model.User.Email</div>
            </div>
            <div class="list-group-item d-flex justify-content-start">
                <div class="px-3 mx-3">Пароль: @Model.User.Password</div>
            </div>
            <div class="list-group-item d-flex justify-content-start">
                <div class="px-3 mx-3">Номер телефона: @Model.User.PhoneNumber</div>
            </div>
            <div class="list-group-item d-flex justify-content-start">
                <div class="px-3 mx-3">Адрес: @Model.User.Address</div>
            </div>
        </div>
    </div>
</div>